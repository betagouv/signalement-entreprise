 # Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /api/companies/search/:identity        controllers.EtablissementController.searchEtablissementByIdentity(identity: String, openOnly :Option[Boolean])
GET         /api/companies/search                  controllers.EtablissementController.searchEtablissement(q: String, postalCode: String)
POST        /api/companies/search                  controllers.EtablissementController.getBySiret()
POST        /api/companies/import                  controllers.EtablissementController.importEtablissements()